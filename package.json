{
  "name": "turfmapp",
  "version": "1.0.0",
  "description": "Turfmapp frontend",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rimraf dist",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ./app/**/**.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "precommit": "cross-env npm run lint && npm test",
    "prepush": "cross-env npm run lint && npm test"
  },
  "author": "Mrinal Mech <mrinalmech@gmail.com>",
  "jest": {
    "setupFiles": [
      "<rootDir>/shim.js"
    ],
    "mapCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ]
  },
  "dependencies": {
    "add": "^2.0.6",
    "classnames": "^2.2.5",
    "cross-env": "5.1.1",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "material-ui": "^0.20.0",
    "moment": "^2.19.4",
    "prop-types": "^15.6.0",
    "react": "16.1.1",
    "react-beautiful-dnd": "^3.0.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "16.1.1",
    "react-dropdown": "^1.3.3",
    "react-dropzone": "^4.2.3",
    "react-hot-loader": "3.1.2",
    "react-number-format": "^3.1.2",
    "react-redux": "^5.0.6",
    "react-responsive-modal": "^2.0.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "4.2.0",
    "react-select": "^1.1.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-form-material-ui": "^4.3.3",
    "redux-thunk": "^2.2.0",
    "yarn": "^1.3.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "copy-webpack-plugin": "4.1.1",
    "css-loader": "0.28.7",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-loader": "1.8.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.0",
    "eslint-plugin-react": "7.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.4",
    "html-webpack-plugin": "2.30.0",
    "husky": "0.15.0-beta.2",
    "jest": "21.2.0",
    "node-sass": "4.7.1",
    "open-browser-webpack-plugin": "0.0.5",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.1.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "url-loader": "0.6.1",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4"
  }
}
